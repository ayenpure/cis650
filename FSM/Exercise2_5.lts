range T 0..30

DRINKS = DRINKS[0],
DRINKS[c:T] = (when(c >= 15) deliver -> CHANGE[c-15]
               | in[5] -> DRINKS[c+5]
               | in[10] -> DRINKS[c+10]
               | in[20] -> DRINKS[c+20]
              ),
CHANGE[c:0..15] = (when(i >= 5 && i < 10) out[5] -> CHANGE[i-5]
                   | when(i >= 10 && i < 20) out[10] -> CHANGE[i-10]
                   | when(i >= 20) out[20] -> CHANGE[i-20] 
                   | when(i == 0) reset -> DRINKS[0]
                  ).
